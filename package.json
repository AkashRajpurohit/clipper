{
  "name": "clipper",
  "version": "1.0.0",
  "description": "Save history of all information you copy and use them whenever with a solitary snap",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-builder",
    "package:win": "electron-builder -w",
    "package:linux": "electron-builder -l",
    "package:all": "electron-builder -lw"
  },
  "repository": "https://github.com/AkashRajpurohit/clipper",
  "keywords": [
    "Electron",
    "Clipper",
    "Copy to clipboard"
  ],
  "author": "Akash Rajpurohit <akash009.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.0.1",
    "electron-builder": "^22.3.2"
  }
}
